{
  "schemaVersion": 1,
  "id": "auto-reauth",
  "version": "${version}",
  "name": "auto-reauth",
  "description": "Automatically re-authenticates your session when it expires",
  "icon": "assets/icon.png",
  "authors": [
    "Connor Slade"
  ],
  "contact": {
    "sources": "https://github.com/Basicprogrammer10/auto-reauth",
    "homepage": "https://github.com/Basicprogrammer10/auto-reauth",
    "issues": "https://github.com/Basicprogrammer10/auto-reauth/issues"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.connorcode.autoreauth.Main"
    ],
    "modmenu": [
      "com.connorcode.autoreauth.ModMenu"
    ]
  },
  "mixins": [
    "auto-reauth.mixins.json"
  ],
  "accessWidener": "auto-reauth.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Source Code": "https://github.com/Basicprogrammer10/auto-reauth",
        "My Website": "https://connorcode.com"
      }
    }
  }
}
